<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.ibbl.menu.MenuBean" table="T_MENU">
        <id name="menuID" column="menu_id">
            <generator class="increment">
            </generator>
        </id>

        <property name="menuOrder" column="MENU_ORDER"/>
        <property name="menuText" column="MENU_TEXT"/>
        <property name="menuLink" column="MENU_LINK"/>
        <property name="status" column="STATUS"/>

        <property name="rAppVersion" column="R_APP_VERSION"/>
        <property name="parentID" column="PARENT_ID"/>
        <property name="iconClass" column="ICON_CLASS"/>
        <property name="isPopMenu" column="IS_POP_MENU"/>
        <property name="menuType" column="MENU_TYPE"/>
    </class>
</hibernate-mapping>