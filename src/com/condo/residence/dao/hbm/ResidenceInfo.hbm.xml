<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.condo.residence.bean.ResidenceInfoBean" table="t_residence_details">
        <id name="OID" column="OID">
            <generator class="increment">
            </generator>
        </id>

        <property name="name" column="NAME"/>
        <property name="phoneNo" column="PHONE_NO"/>
        <property name="email" column="EMAIL"/>
        <property name="blockNO" column="BLOCK_NO"/>
        <property name="flatNo" column="FLAT_NO"/>
        <property name="userID" column="USER_ID"/>
        <property name="status" column="STATUS"/>
        <property name="imageUrl" column="IMAGE_URL"/>
    </class>
</hibernate-mapping>